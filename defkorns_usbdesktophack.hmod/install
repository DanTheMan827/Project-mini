local script="S52defkornsdesktophack"
copy "$script" "$rootfs/etc/init.d/"
chmod +x "$rootfs/etc/init.d/$script"
cfg_DefKorns_desktophack_enabled='y'
mv "$rootfs/boxart" "$mountpoint/media/hakchi/boxart"
return 1